    SUBROUTINE ATI.BM.UPD.LLC.FLD.SELECT
*-----------------------------------------------------------------------------
* Developer Name     : 20170713
* Development Date   : Novi Leo
* Description        : Routine to update from old LLC field to new LLC field
*-----------------------------------------------------------------------------
* Modification History:-
*-----------------------------------------------------------------------------
* Date               :
* Modified by        :
* Description        :
*-----------------------------------------------------------------------------

    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT I_ATI.BM.UPD.LLC.FLD.COMMON

*-----------------------------------------------------------------------------
MAIN:
*-----------------------------------------------------------------------------

    GOSUB INIT
    GOSUB PROCESS

    RETURN
*-----------------------------------------------------------------------------
INIT:
*-----------------------------------------------------------------------------


    RETURN
*-----------------------------------------------------------------------------
PROCESS:
*-----------------------------------------------------------------------------

    SEL.CMD = "SELECT ":FN.CUSTOMER:" WITH LLC.Y.N EQ 'Y'"
    CALL EB.READLIST(SEL.CMD,SEL.LIST,"",SEL.NO,SEL.ERR)

    CALL BATCH.BUILD.LIST("",SEL.LIST)

    RETURN
*-----------------------------------------------------------------------------
END










