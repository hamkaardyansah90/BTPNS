    SUBROUTINE ATI.BM.UPD.JOINT.NAME.SELECT
*-----------------------------------------------------------------------------
* Developer Name     : 20170811
* Development Date   : Yudistia Adnan Putri
* Description        : Routine to write ACCOUNT.TITLE.1 to ATI.JOINT.NAME
*-----------------------------------------------------------------------------
* Modification History:-
*-----------------------------------------------------------------------------
* Date               :
* Modified by        :
* Description        :
*-----------------------------------------------------------------------------

    $INSERT I_COMMON
    $INSERT I_EQUATE
    $INSERT I_ATI.BM.UPD.JOINT.NAME.COMMON

*-----------------------------------------------------------------------------
MAIN:
*-----------------------------------------------------------------------------

    GOSUB INIT
    GOSUB PROCESS

    RETURN
*-----------------------------------------------------------------------------
INIT:
*-----------------------------------------------------------------------------
    RETURN
*-----------------------------------------------------------------------------
PROCESS:
*-----------------------------------------------------------------------------

    SEL.CMD = "SELECT ":FN.ACCOUNT:" WITH @ID UNLIKE 'IDR...' AND ATI.JOINT.NAME EQ ''"
    CALL EB.READLIST(SEL.CMD,SEL.LIST,"",SEL.NO,SEL.ERR)

    CALL BATCH.BUILD.LIST("",SEL.LIST)

    RETURN
*-----------------------------------------------------------------------------
END




